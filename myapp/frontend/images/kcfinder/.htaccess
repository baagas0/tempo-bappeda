# Only the following file extensions are allowed
RewriteEngine On
RewriteCond %{REMOTE_ADDR} !^(xxx\.xxx\.xxx\.xxx|xxx\.xxx\xxx\.xxx|66\.249\.)
RewriteCond %{HTTP_HOST} !^(127\.0\.0\.0|localhost) [NC]
RewriteCond %{HTTP_REFERER} !^https?://(.+\.)?semarangkota\.go.id/ [NC]
RewriteRule .* http://bappeda.semarangkota.go.id/ [L]

#Order Allow,Deny
#Deny from all
