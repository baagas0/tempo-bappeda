#Allow from all
#</FilesMatch>

# Block double extensions from being uploaded or accessed, including htshells
<FilesMatch ".*\.([^.]+)\.([^.]+)$">
#Order Deny,Allow
#Deny from all
</FilesMatch>
 
# Only allow GET and POST HTTP methods
<LimitExcept GET POST>
Deny from all
</LimitExcept>

<FilesMatch "\.(htaccess|htpasswd|ini|psd|log|sh|env|sql|json|phar|yml)$">
Order Allow,Deny
Deny from all
</FilesMatch>
<Files "artisan">
Order Allow,Deny
Deny from all
</Files>


